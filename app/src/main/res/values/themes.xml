<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Dule" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue2</item>
        <item name="colorPrimaryVariant">@color/blue2</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/bluelight</item>
        <item name="colorSecondaryVariant">@color/bluelight</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>


        <!-- Customize your theme here. -->
    </style>

    <style name="TextViewInter_regular20">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextViewInter_regular16">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextViewInter_regular16white">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextViewInter_bold20">
        <item name="android:fontFamily">@font/inter_bold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="TextViewInter_bold20White">
        <item name="android:fontFamily">@font/inter_bold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextViewInter_semibold20">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/blackgrey</item>
    </style>

    <style name="ButtonCalendar" parent="@style/Widget.MaterialComponents.Button">
        <item name="fontFamily">@font/inter_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingHorizontal">10dp</item>



    </style>
    
    <style name="accent" parent="ThemeOverlay.Material3.MaterialCalendar">
        <item name="android:colorAccent">@color/blue</item>
        <item name="android:textAppearance">@font/inter_semibold</item>

    </style>


</resources>